{
  "compileOnSave": false,
  "compilerOptions": {
    "plugins": [],
    "baseUrl": "./",
    "outDir": "./",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "lib": ["es2018", "dom", "esnext.array"]
  },
  "include": [
    "./*.ts"
  ],
  "exclude": [],
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
